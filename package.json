{
	"name": "@incutonez/fabled-basin",
	"description": "Fabled Basin Phaser 3 Game",
	"version": "2.1.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/incutonez/fabled-basin.git"
	},
	"author": "incutonez@hotmail.com",
	"license": "MIT",
	"licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
	"keywords": [
		"phaser",
		"phaser3",
		"typescript",
		"vue",
		"vite"
	],
	"scripts": {
		"dev": "vite --config vite/config.dev.ts",
		"build": "vite build --config vite/config.prod.ts",
		"explode": "npm cache clean --force && npx rimraf package-lock.json **/node_modules --glob && npm i",
		"prepare": "husky"
	},
	"dependencies": {
		"@incutonez/core-ui": "^3.0.13",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"lodash-es": "^4.17.21",
		"phaser": "^3.88.2",
		"reflect-metadata": "^0.2.2",
		"vue": "^3.5.13",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.23.0",
		"@incutonez/eslint-plugin": "^2.1.4",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^7.0.3",
		"@semantic-release/git": "^10.0.1",
		"@stylistic/eslint-plugin-ts": "^4.2.0",
		"@tailwindcss/vite": "^4.0.12",
		"@tsconfig/node20": "^20.1.4",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.13.10",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/tsconfig": "^0.7.0",
		"conventional-changelog-conventionalcommits": "^8.0.0",
		"eslint": "^9.22.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-vue": "^10.0.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.4.3",
		"semantic-release": "^24.2.3",
		"tailwindcss": "^4.0.12",
		"terser": "^5.39.0",
		"typescript": "~5.8.2",
		"typescript-eslint": "^8.26.1",
		"vite": "^6.2.1",
		"vue-tsc": "^2.2.8"
	},
	"lint-staged": {
		"*.{js,mjs,cjs,jsx,ts,tsx,vue}": [
			"npx eslint --fix"
		]
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			[
				"@semantic-release/commit-analyzer",
				{
					"preset": "conventionalcommits"
				}
			],
			[
				"@semantic-release/release-notes-generator",
				{
					"preset": "conventionalcommits"
				}
			],
			"@semantic-release/changelog",
			[
				"@semantic-release/npm",
				{
					"npmPublish": false
				}
			],
			"@semantic-release/git",
			"@semantic-release/github"
		]
	}
}
